
<div class="help">
  <%= form_for :event, url: events_path do |f| %>
    <p class="getuser"> 
      User: <%= f.select :user_id, allusers %>    
    </p>
    <p>
      <button class="getcats">Get Cats from User</button>
    </p>
    <p>
      <%= f.label :start %>
      <%= f.date_field :start %>
    </p>
    <p>
      <%= f.label :end %>
      <%= f.date_field :end %>
    </p>
    <p>
      <%= f.label :price %>
      <%= f.number_field :price %>
    </p>
    <p>
      <%= f.submit "Create Event" %>
    </p>
  <% end %>
</div>

